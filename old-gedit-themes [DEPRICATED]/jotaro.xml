<?xml version="1.0" ?>
<style-scheme id="jotaro" name="Jotaro    承太郎    Monospace Regular 12" version="1.0">
	<author>Djordje Stankovic</author>
	<_description>Najbolja koja ima da je tema! {Monospace Regular 12}</_description>
	<style background="#151315" foreground="#ffffff" name="text"/> <!--  Background = CEELA pozadina!  -->
	<style background="#ffae00" foreground="#800000" name="search-match"/> <!--  CTRL-F  -->
	<style background="#1b1b1b" name="current-line"/> <!--  Color of the highlighted line  -->
	<style background="#151315" foreground="#dadada" name="line-numbers"/> <!--  Brojevi levo   -->
	
	<style bold="true" foreground="#1cff00" name="def:function"/> <!--  ?  -->
	<style foreground="#20B2AA" name="def:specials"/> <!--  ?  -->
	<style foreground="#20B2AA" name="def:constant"/> <!--  NULL, PI u C-u  -->
	<style bold="true" foreground="#ff0086" name="def:number"/> <!--  Svi brojevi u C-u  -->
	
	<style bold="true" foreground="#e00947" name="def:statement"/> <!--  CEO ASEMBLER, for, while, if, return  -->
	<style bold="true" foreground="#2ccad7" name="def:preprocessor"/> <!--  .section, #include  -->
	<style foreground="#ff73ff" name="def:string"/> <!-- 9b73ff SVE POD NAVODNICIMA ba6fff -->
	
	<style background="#1c1c1c" bold="true" foreground="#ff0086" name="bracket-match"/> <!--  Self explanatory  -->
	<style background="#ff0000" bold="true" foreground="#ffffff" name="bracket-mismatch"/>
	
	<style background="#151315" foreground="#00bccb" italic="true" name="def:comment"/> <!--  Background = isto kao za tekst, foreground = boja komentara  -->
	<style background="#151515" foreground="#00bccb" italic="true" name="def:shebang"/> <!--  ?  -->
	<style bold="true" foreground="#00cbff" name="def:type"/> <!--  Sve sto je ovde background, foreground, registar  -->
	<style background="#ff0000" foreground="#ffffff" name="def:error"/> <!--  Boja greske , proveri kad razdvojis prvi komentar  -->
	<style bold="true" foreground="#ff0086" name="def:identifier"/> <!--  <style> je ove boje  -->
	
	<style foreground="#ff7272" background="#00000f" name="selection"/>
	
	<style foreground="#20B2AA" name="def:special-char"/> <!-- %d u printf-u -->
	<style foreground="#00cbff" bold="true" name="cursor"/> <!-- Insert i obican kursor u editoru -->
</style-scheme>
